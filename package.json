{
  "name": "b5g",
  "version": "0.0.3",
  "description": "Convenience package to generate templates around Bve5 syntax",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "files": [
    "out"
  ],
  "scripts": {
    "build": "tsc && npm run cpdef",
    "prepare": "npm run build",
    "cpdef": "cpx src/mapgrammar.yaml ./out",
    "cpdef_watch": "cpx src/mapgrammar.yaml ./out --watch",
    "test": "jest",
    "lint": "tslint 'src/**/*.ts'"
  },
  "author": "aoisupersix",
  "license": "MIT",
  "keywords": [
    "bve5"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aoisupersix/B5G"
  },
  "dependencies": {
    "fs": "0.0.2",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "linq": "^3.2.2",
    "mustache": "^4.0.1",
    "strip-bom": "^4.0.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.1",
    "@types/jest": "25.2.3",
    "@types/js-yaml": "3.12.4",
    "@types/linq": "2.2.33",
    "@types/mustache": "4.0.1",
    "@types/node": "14.0.12",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "chokidar": "3.4.0",
    "coveralls": "3.1.0",
    "cpx": "1.5.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "26.1.0",
    "js-yaml": "3.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "26.1.0",
    "ts-node": "8.10.2"
  }
}
