{
  "name": "b5g",
  "version": "0.0.3",
  "description": "Convenience package to generate templates around Bve5 syntax",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "files": [
    "out"
  ],
  "scripts": {
    "build": "tsc && npm run cpdef",
    "prepare": "npm run build",
    "cpdef": "cpx src/mapgrammar.yaml ./out",
    "cpdef_watch": "cpx src/mapgrammar.yaml ./out --watch",
    "test": "jest",
    "lint": "tslint 'src/**/*.ts'"
  },
  "author": "aoisupersix",
  "license": "MIT",
  "keywords": [
    "bve5"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aoisupersix/B5G"
  },
  "dependencies": {
    "fs": "0.0.2",
    "fs-extra": "^8.1.0",
    "linq": "^3.2.0",
    "mustache": "^3.0.1",
    "strip-bom": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/fs-extra": "8.0.0",
    "@types/jest": "24.0.15",
    "@types/js-yaml": "3.12.1",
    "@types/linq": "2.2.33",
    "@types/mustache": "0.8.32",
    "@types/node": "12.0.0",
    "chokidar": "3.0.2",
    "coveralls": "3.0.5",
    "cpx": "1.5.0",
    "jest": "24.8.0",
    "js-yaml": "3.13.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tsutils": "3.10.0"
  }
}
