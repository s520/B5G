{
  "name": "b5g",
  "version": "0.0.3",
  "description": "Convenience package to generate templates around Bve5 syntax",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "files": [
    "out"
  ],
  "scripts": {
    "build": "tsc && npm run cpdef",
    "prepare": "npm run build",
    "cpdef": "cpx src/mapgrammar.yaml ./out",
    "cpdef_watch": "cpx src/mapgrammar.yaml ./out --watch",
    "test": "jest",
    "lint": "tslint 'src/**/*.ts'"
  },
  "author": "aoisupersix",
  "license": "MIT",
  "keywords": [
    "bve5"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aoisupersix/B5G"
  },
  "dependencies": {
    "fs": "0.0.2",
    "fs-extra": "^9.0.0",
    "handlebars": "^4.7.2",
    "linq": "^3.2.0",
    "mustache": "^4.0.0",
    "strip-bom": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.1",
    "@types/jest": "25.2.3",
    "@types/js-yaml": "3.12.4",
    "@types/linq": "2.2.33",
    "@types/mustache": "4.0.0",
    "@types/node": "12.12.34",
    "chokidar": "3.4.0",
    "coveralls": "3.0.11",
    "cpx": "1.5.0",
    "jest": "26.0.1",
    "js-yaml": "3.13.1",
    "ts-jest": "26.0.0",
    "ts-node": "8.8.1",
    "tslint": "6.1.2",
    "tslint-config-airbnb": "5.11.2",
    "tsutils": "3.17.1"
  }
}
